#include<iostream>
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {  
        string n;
        cin>>n;
        int k;
        cin>>k;
  
        for(int i=0;i<k;i++)
        {       sort(n.begin(),n.end());
            if(n[0]=='9')
            break;
            n[0]=n[0]+1;
        }
       
       long long int sum =1;
      for(int i=0;i<n.size();i++)
      {
          sum=(sum*(n[i]-'0'));
      }
       cout<<sum<<endl;
        
    }
    
    return 0;
}
